{
    "name": "cypress-allure-plugin-example",
    "version": "1.0.0",
    "main": "index.js",
    "license": "Apache-2.0",
    "scripts": {
        "cy:open": "npx cypress open --env allure=true --spec 'cypress/e2e/usuario/*.cy.js' --browser chrome",
        "cy:run": "DEBUG=allure-plugin* npx cypress run --spec 'cypress/e2e/usuario/*.cy.js' --env allure=true --browser chrome",
        "allure:report": "allure generate allure-results --clean -o allure-report",
        "allure:clear": "rm -r allure-results/ allure-report cypress/screenshots || true",
        "allure:history": "mv -f allure-report/history allure-results/history && rm -r allure-report || true"
    },
    "devDependencies": {
        "@cypress/webpack-preprocessor": "6.0.4",
        "@shelex/cypress-allure-plugin": "2.41.0",
        "cypress": "14.2.1",
        "cypress-plugin-api": "2.11.2",
        "eslint-plugin-chai-friendly": "1.0.1",
        "eslint-plugin-cypress": "4.2.0"
    }
}